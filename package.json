    "type": "git",
    "url": "git+https://github.com/dboikliev/LinqTS.git"
  },
  "bugs": {
    "url": "https://github.com/dboikliev/LinqTS/issues"
  },
  "homepage": "https://github.com/dboikliev/LinqTS#readme"
}
