{
  "name": "linqts",
  "version": "1.0.0",
  "description": "An api for lazy querying of iterables, implemented in TypeScript and inspired by .NET's LINQ methods.",
  "main": "app.js",
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "@types/node": "^8.10.66",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "istanbul": "^0.4.5",
    "mocha": "^8.2.1",
    "mocha-lcov-reporter": "^1.3.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "build": "rimraf ./build && tsc -p .",
    "test-mocha": "mocha ./build/test/**/*.spec.js",
    "test": "npm run build && npm run test-mocha",
    "cover": "./node_modules/.bin/istanbul cover _mocha build/test/**/*.spec.js",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "lint": "eslint \"./src/**/*.ts\"",
    "lint-fix": "eslint ./**/*.ts --fix"
  },
